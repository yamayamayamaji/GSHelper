{
  "name": "GSHelper",
  "version": "0.3.0.0",
  "description": "helper extension for GroupSession",
  "content_scripts": [ {
      "all_frames": true,
      "matches": [
        "*://*/gsession/*",
        "*://*/gsession3/*"
      ],
      "js": ["jquery.js", "content.js"]
  } ],
  "manifest_version": 2,
  "permissions": [
    "*://*/gsession/*",
    "*://*/gsession3/*"
  ],
  "web_accessible_resources": ["manifest.json"]
}
