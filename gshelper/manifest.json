{
  "name": "GSHelper",
  "version": "0.3.4.0",
  "description": "グループウェア Group Session (http://www.gs.sjts.co.jp/) にちょっと機能を付け足します。",
  "content_scripts": [ {
      "all_frames": true,
      "matches": [
        "*://*/gsession/*",
        "*://*/gsession3/*"
      ],
      "js": ["jquery.js", "content.js"]
  } ],
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "*://*/gsession/*",
    "*://*/gsession3/*"
  ],
  "web_accessible_resources": [
    "manifest.json",
    "content.css"
  ]
}
